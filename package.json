  "engines": {
    "node": ">=v16.13.1 <17"
  },
  "os": [
    "linux",
    "darwin",
    "!win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "build-dev": "webpack --watch --config webpack.dev.config.js",
    "webpack-serve-dev": "webpack serve --config webpack.dev.config.js",
    "build-prod.watch": "webpack --watch --config webpack.prod.config.js",
    "webpack-serve": "webpack serve --config webpack.prod.config.js",
    "nodemon.koorie-cluster-react": "npx nodemon --trace-warnings index.js --cluster --static-files=public --port=2654 --address=localhost --react",
    "build-prod": "webpack --config webpack.prod.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
