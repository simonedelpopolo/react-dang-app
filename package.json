{
  "name": "@react-dang/app",
  "version": "0.3.4-alpha",
  "description": "basic react-dang-app",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "dang-serve": "./executable.js"
  },
  "scripts": {
    "eslint": "npx eslint . --ext .js",
    "eslint-fix": "npx eslint --fix . --ext .js",
    "build-dev": "webpack --watch --config webpack.dev.config.js",
    "serve-dev": "webpack serve --config webpack.dev.config.js",
    "build-prod": "webpack --config webpack.prod.config.js",
    "build-prod.watch": "webpack --watch --config webpack.prod.config.js",
    "serve": "webpack serve --config webpack.prod.config.js",
    "nodemon.server-alpha-cluster-json": "npx nodemon executable.js -c='{\"cpus\":8,\"init\":\"default\"}' --static-files=public --port=2654 --address=localhost",
    "server-alpha-cluster": "node executable.js --cluster --port=2653 --address=localhost --static-files=public",
    "server-alpha-cluster-json": "node executable.js -c='{\"cpus\":8,\"init\":\"default\"}' --static-files=public --port=2654 --address=localhost",
    "server-alpha": "node executable.js --port=8001 --address=localhost --static-files=public",
    "serve.server-alpha": "node executable.js --static-files=public"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-plugin-jsdoc": "^37.6.1",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.6",
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "dependencies": {
    "boolean-jokes": "^1.4.5",
    "json-swiss-knife": "^1.5.7",
    "oftypes": "^0.3.1-beta",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "author": "Simone Del Popolo",
  "license": "Apache-2.0"
}
